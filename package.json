{
  "name": "cilium.io",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Cilium",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "lint": "npm run lint:js & npm run lint:md",
    "lint:fix": "npm run lint:js:fix & npm run lint:md:fix",
    "lint:js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:js:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "markdownlint  --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix  --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^6.2.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "classnames": "^2.3.1",
    "gatsby": "^4.5.4",
    "gatsby-plugin-feed": "^4.5.0",
    "gatsby-plugin-google-tagmanager": "^4.5.0",
    "gatsby-plugin-hotjar": "^1.1.1",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-mdx": "^3.5.2",
    "gatsby-plugin-netlify": "^4.0.0",
    "gatsby-plugin-sitemap": "^5.5.0",
    "gatsby-remark-copy-linked-files": "^5.5.0",
    "gatsby-remark-images": "^6.5.2",
    "gatsby-remark-prismjs": "^6.5.0",
    "gatsby-remark-responsive-iframe": "^5.5.0",
    "gatsby-remark-video": "^1.2.5",
    "prismjs": "^1.26.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-calendly": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-share": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-alias-imports": "^1.0.6",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-offline": "^5.5.2",
    "gatsby-plugin-postcss": "^5.5.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "markdownlint-cli": "^0.30.0",
    "node-fetch": "^2.6.7",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.15"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isovalent/cilium.io-v2"
